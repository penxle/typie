{
  "name": "@typie/fondue",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "build:napi": "bun run build:napi:aarch64-apple-darwin && bun run build:napi:aarch64-unknown-linux-gnu",
    "build:napi:aarch64-apple-darwin": "napi build --release --output-dir=out --platform --target=aarch64-apple-darwin",
    "build:napi:aarch64-unknown-linux-gnu": "napi build --release --output-dir=out --platform --target=aarch64-unknown-linux-gnu --cross-compile"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.1.5"
  },
  "napi": {
    "binaryName": "fondue",
    "targets": [
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu"
    ]
  }
}
