{
  "manifest_version": 2,
  "name": "extension",
  "version": "1.22",
  "browser_specific_settings": {
    "gecko": {
      "id": "extension@typie.co"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end",
      "match_about_blank": true,
      "match_origin_as_fallback": true
    }
  ],
  "permissions": [
    "activeTab",
    "cookies",
    "geckoViewAddons",
    "nativeMessaging",
    "nativeMessagingFromContent",
    "<all_urls>"
  ]
}