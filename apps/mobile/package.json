{
  "name": "@typie/mobile",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "flutter clean && dart run build_runner clean && pnpm clean:generated",
    "clean:generated": "find . -name '__generated__' -type d -print -delete",
    "codegen:build": "rps -- dart run build_runner build --delete-conflicting-outputs",
    "codegen:watch": "rps -- dart run build_runner watch --delete-conflicting-outputs"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.62",
    "@iconify-json/lucide-lab": "^1.2.3",
    "@iconify/tools": "^4.1.2",
    "@typie/rps": "workspace:*",
    "change-case-all": "^2.1.0",
    "fantasticon": "^3.0.0",
    "oslllo-svg-fixer": "^5.0.0"
  }
}
